// Assign Liquibase version
// def LIQUIBASE_VERSION = "4.8.0"

pipeline {
    agent any
    options {
        ansiColor('xterm')
    }

    stages {
        
        stage("Install Liquibase") {
            steps {

                //Display installed Java version
                sh('java -version')
                
                sh('python -h')
                sh('python -V')
                sh('python3 --verison')
                
            }
        } //stage

        
    } //stages
} //pipeline
